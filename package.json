{
  "name": "@gplane/svelte-jest",
  "version": "0.1.0",
  "author": "Pig Fang <g-plane@hotmail.com>",
  "description": "Jest custom transformer for transforming Svelte.",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "prepublishOnly": "yarn build",
    "typeck": "tsc -p . --noEmit && tsc -p ./tests --noEmit",
    "fmt": "prettier --write **/*.ts **/*.svelte",
    "fmt:check": "prettier --check **/*.ts **/*.svelte",
    "pretest": "yarn build",
    "test": "jest"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "svelte",
    "jest",
    "jest-transformers",
    "testing-library"
  ],
  "peerDependencies": {
    "svelte": "^3.24.0",
    "svelte-preprocess": "^4.1.0"
  },
  "devDependencies": {
    "@gplane/tsconfig": "^2.0.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "svelte": "^3.24.1",
    "svelte-preprocess": "^4.1.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
